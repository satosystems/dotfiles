{
    "files.insertFinalNewline": true,
    "arduino.additionalUrls": [
        "http://arduino.esp8266.com/stable/package_esp8266com_index.json"
    ],
    "diffEditor.ignoreTrimWhitespace": true,
    "window.zoomLevel": 0,
    "html.format.endWithNewline": true,
    "editor.renderControlCharacters": true,
    "languageServerHaskell.liquidOn": true,
    "extensions.ignoreRecommendations": false,
    "editor.fontFamily": "Ricty Discord",
    "editor.fontSize": 16,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent",
    "editor.quickSuggestions": true,
    "editor.largeFileOptimizations": false,
    "editor.formatOnSave": true,
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.formatOnSave": false
    },
    "haskell.hlint.executablePath": "hlint",
    "haskell.hlint.run": "onSave",
    "javascript.suggestionActions.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "C_Cpp.updateChannel": "Insiders",
    "indentRainbow.ignoreErrorLanguages": [
        "haskell"
    ],
    "eslint.alwaysShowStatus": true,
    "eslint.provideLintTask": true,
    "eslint.packageManager": "yarn",
    "window.nativeTabs": true,
    "cmake.configureOnOpen": true,
    "cmake.cmakePath": "/usr/local/bin/cmake",
    "cpplint.cpplintPath": "~/.pyenv/shims/cpplint",
    "cpplint.filters": [
        "-build/include_order",
        "-whitespace/line_length"
    ],
    "glsllint.glslangValidatorPath": "~/Library/glslang/bin/glslangValidator",
    "[glsl]": {
        "editor.defaultFormatter": "circledev.glsl-canvas"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "eslint.enable": true,
    "eslint.run": "onType",
    "editor.rulers": [
        80
    ],
    "terminal.integrated.fontSize": 14,
    "files.associations": {
        "*.hs": "haskell"
    },
    "brittany.keepCRLF": true
}
