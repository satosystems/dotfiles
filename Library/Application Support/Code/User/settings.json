{
  "plantuml.java": "/usr/local/opt/openjdk/bin/java",
  "brittany.keepCRLF": true,
  "editor.fontFamily": "Ricty Discord",
  "editor.rulers": [
    80
  ],
  "editor.tabSize": 2,
  "files.associations": {
    "*.hs": "haskell",
    "*.aiml": "xml"
  },
  "haskell.hlint.executablePath": "hlint",
  "haskell.hlint.run": "onSave",
  "indentRainbow.ignoreErrorLanguages": [
    "haskell"
  ],
  "javascript.validate.enable": false,
  "standard.enable": true,
  "standard.run": "onType",
  "standard.autoFixOnSave": true,
  "standard.validate": [
    "javascript",
    "html"
  ],
  "standard.options": {
    "plugins": [
      "html"
    ],
    "ignore": [
      "node_modules/**"
    ],
  },
  "typescript.suggestionActions.enabled": false,
  "javascript.suggestionActions.enabled": false,
  "languageServerHaskell.liquidOn": true,
  "languageServerHaskell.useCustomHieWrapper": true,
  "languageServerHaskell.useCustomHieWrapperPath": "hie-wrapper",
  "[haskell]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "MaxGabriel.brittany"
  },
  "[css]": {
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.formatOnSave": true
  },
  // "[js]": {
  //   "editor.formatOnSave": false
  // },
  "yaml.format.enable": true,
  "[yml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontSize": 14,
  "window.zoomLevel": 0,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  "editor.fontSize": 14,
  "terminal.integrated.scrollback": 1000000,
  "google-translate.firstLanguage": "ja-jp",
  "google-translate.serverDomain": "https://translate.google.com",
  "azure.resourceFilter": [
    "8f5e020a-eadb-4caa-811c-b4ee629eb1ba/e2a2626e-f347-495a-8b74-86f7f64161e3"
  ],
  "hediet.vscode-drawio.local-storage": "",
  "appService.showSavePrompt": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)"
}